{
  "name": "ember-functions-as-helper-polyfill",
  "version": "1.0.14",
  "description": "Implementation of RFC 756, Default Helper Manager",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/NullVoxPopuli/ember-functions-as-helper-polyfill",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "@embroider/macros": "^1.0.0",
    "@glimmer/tracking": "^1.0.4",
    "ember-auto-import": "^2.4.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-typescript": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@ember/test-waiters": "^3.0.1",
    "@embroider/test-setup": "^1.0.0",
    "@nullvoxpopuli/eslint-configs": "^2.1.18",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/ember-qunit": "^3.4.15",
    "@types/ember-resolver": "^5.0.11",
    "@types/ember__application": "^3.16.4",
    "@types/ember__array": "^3.16.5",
    "@types/ember__component": "^3.16.7",
    "@types/ember__controller": "^3.16.7",
    "@types/ember__debug": "^3.16.6",
    "@types/ember__engine": "^3.16.4",
    "@types/ember__error": "^3.16.2",
    "@types/ember__object": "^3.12.7",
    "@types/ember__polyfills": "^3.12.2",
    "@types/ember__routing": "^3.16.16",
    "@types/ember__runloop": "^3.16.4",
    "@types/ember__service": "^3.16.2",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^3.16.2",
    "@types/ember__test": "^3.16.2",
    "@types/ember__test-helpers": "^2.6.1",
    "@types/ember__utils": "^3.16.3",
    "@types/htmlbars-inline-precompile": "^1.0.1",
    "@types/qunit": "^2.11.3",
    "@types/rsvp": "^4.0.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.4.0",
    "ember-cli": "~4.1.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "4.1.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-decorator-position": "^4.0.1",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "semantic-release": "^19.0.2",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0"
  },
  "release": {
    "branches": [
      "main",
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "volta": {
    "node": "16.13.2",
    "yarn": "1.22.17"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-could-get-used-to-this"
    ]
  }
}
